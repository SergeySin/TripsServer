<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="hiber_part.SourceAssociationsEntity" table="Source_associations" schema="mydb">
        <id name="saId">
            <column name="SA_id" sql-type="int(11)"/>
        </id>
        <property name="saName">
            <column name="SA_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="saSourceId">
            <column name="SA_Source_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>